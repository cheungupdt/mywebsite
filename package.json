{
  "name": "kcsteam-website",
  "version": "1.0.0",
  "description": "GitHub Actions auto-deployment to Netlify",
  "main": "index.html",
  "scripts": {
    "start": "eleventy --serve --port=8080",
    "build": "eleventy --quiet",
    "build:production": "eleventy --quiet",
    "serve": "eleventy --serve",
    "dev": "eleventy --serve --watch",
    "deploy": "netlify deploy --prod --dir=dist",
    "deploy:preview": "netlify deploy --preview"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "chart.js": "^4.4.0",
    "mermaid": "^10.6.0",
    "three": "^0.150.0"
  },
  "devDependencies": {
    "netlify-cli": "^17.0.0",
    "clean-css": "^5.3.2",
    "html-minifier-terser": "^7.2.0"
  }
}
